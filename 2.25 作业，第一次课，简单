#include<stdio.h>//输出绝对值
int main()
{
	int a;
	scanf("%d",&a);
	if(a<=0){
		a=-a;
	}
	printf("%d",a);
	return 0;
} 

#include<stdio.h>//温度转换
int main()
{
	int c;
	double f;
	scanf("%d",&c);
	printf("%f",9*c/5.0+32);
	return 0;
} 

#include<stdio.h>//体积计算
int main()
{
	float r,h,S,V;
	const double pi=3.14;
	scanf("%f %f",&r,&h);
	S=pi*r*r;
	V=S*h;
	printf("area=%.2f,volume=%.2f",S,V);
	return 0;
} 

#include<stdio.h>//判断素数
#include<math.h>
int main()
{
	int n,ret=1;
	scanf("%d",&n);
	for(int i=2;i<=(int)sqrt(double(n));i++){
		if(n%i==0){
			ret=0;
			printf("no");
			break;
		}
	}
	if(ret=1)
	printf("yes");
	return 0;
} 

#include<stdio.h>//冒泡排序
int main()
{
	int a[10],i,j,t;
	for(i=0;i<10;i++){
		scanf("%d",&a[i]);
	}
	for(i=0;i<9;i++){
		for(j=i+1;j<10;j++){
			if(a[i]>a[j]){
				t=a[i];
				a[i]=a[j];
				a[j]=t;
			}
		}
	}
	for(i=0;i<10;i++){
		printf("%d,",a[i]);
	}
	return 0;
} 

#include<stdio.h>//输出e的x次方
int main()
{
	double x,n,sum=1.0,t,b=1.0;
	int i;
	scanf("%lf %lf",&x,&n);
	t=x;
	if(n>=0){
		for(i=1;i<=n;i++){
			sum=sum+t/b;
			t=x*t;
			b=b*(i+1);
		}
		printf("%.6f",sum);
	}
	else
	printf("error");
	return 0;
}
