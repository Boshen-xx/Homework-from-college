#include<stdio.h>//中间数
int main()
{
	int N;
	scanf("%d",&N);
	while(N>0){
		int a,b,c,t;
		scanf("%d%d%d",&a,&b,&c);
		if(a>b){
			t=a;a=b;b=t;
		}
		if(a>c){
			t=a;a=c;c=t;
		}
		if(b>c){
			t=b;b=c;c=t;
		}
		if(a!=b&&b!=c){
			printf("%d\n",b);
		}
		else{
			printf("%d\n",c);
		}
		N--;
	}
	return 0;
}

#include<stdio.h>//跳水打分求平均
int main()
{
	int T;
	scanf("%d",&T);
	while(T--){
		int n;
		int i;
		scanf("%d",&n);
		double a[100];
		double max,min,sum=0;
		for(i=0;i<n;i++){
			scanf("%lf",&a[i]);
		}
		max=a[0];
		min=a[0];
		sum=a[0];
		for(i=1;i<n;i++){
			if(a[i]>max){
				max=a[i];
			}
			if(a[i]<min){
				min=a[i];
			}
			sum=sum+a[i];
		}
		sum=sum-min-max;
		sum=sum/(n-2);
		printf("%.2f\n",sum);
	}
	return 0;
}
