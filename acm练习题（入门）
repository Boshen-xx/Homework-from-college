#include<stdio.h>//中间数
int main()
{
	int N;
	scanf("%d",&N);
	while(N>0){
		int a,b,c,t;
		scanf("%d%d%d",&a,&b,&c);
		if(a>b){
			t=a;a=b;b=t;
		}
		if(a>c){
			t=a;a=c;c=t;
		}
		if(b>c){
			t=b;b=c;c=t;
		}
		if(a!=b&&b!=c){
			printf("%d\n",b);
		}
		else{
			printf("%d\n",c);
		}
		N--;
	}
	return 0;
}

#include<stdio.h>//跳水打分求平均
int main()
{
	int T;
	scanf("%d",&T);
	while(T--){
		int n;
		int i;
		scanf("%d",&n);
		double a[100];
		double max,min,sum=0;
		for(i=0;i<n;i++){
			scanf("%lf",&a[i]);
		}
		max=a[0];
		min=a[0];
		sum=a[0];
		for(i=1;i<n;i++){
			if(a[i]>max){
				max=a[i];
			}
			if(a[i]<min){
				min=a[i];
			}
			sum=sum+a[i];
		}
		sum=sum-min-max;
		sum=sum/(n-2);
		printf("%.2f\n",sum);
	}
	return 0;
}

#include<stdio.h>//能否重组
#include<string.h>
int main()
{
	int T;
	scanf("%d",&T);
	while(T--){
		int cnta[10]={0,};
		int cntb[10]={0,};
		char a[200],b[200];
		scanf("%s",a);
		scanf("%s",b);
		int lengtha,lengthb;
		int ret=1,i;
		lengtha=strlen(a);
		lengthb=strlen(b);
		for(i=0;i<lengtha;i++){
			cnta[a[i]-'0']++;
		}
		for(i=0;i<lengthb;i++){
			cntb[b[i]-'0']++;
		}
		for(i=1;i<10;i++){
			if(cnta[i]!=cntb[i]){
				ret=0;
				break;
			}
		}
		if(ret==1){
			printf("Yes\n");
		}
		else if(ret==0){
			printf("No\n");
		}
		
		
	}
	return 0;
}

#include<stdio.h>//开灯问题
#include<string.h>
int main()
{
	int T;
	scanf("%d",&T);
	while(T--){
		int n,k;
		int a[1002]={0,};
		scanf("%d%d",&n,&k);
		int i,j;
		for(i=1;i<=k;i++){
			j=1;
			while(i*j<=n){
				a[i*j]++;
				j++;
			}
		}
		for(i=1;i<=n;i++){
			if(a[i]%2==1){
				printf("%d ",i);
			}
		}
		printf("\n");
	}
	return 0;
}

#include<stdio.h>//旋转90度的字符矩阵
#include<string.h>
//ÏÈµ¹×ÅÊä³öµÚÒ»ÁÐ£¬È»ºó»»ÐÐµ¹×ÅÊä³öµÚ¶þÁÐ 
int main()
{
	int T;
	scanf("%d",&T);
	while(T--){
		int n,i,j;
		scanf("%d",&n);
		getchar();
		char a[n][n+1];
		for(i=0;i<n;i++){
			for(j=0;j<n+1;j++){
				scanf("%c",&a[i][j]);
			}
		}
		for(i=0;i<n;i++){
			for(j=0;j<n;j++){
				printf("%c",a[n-j-1][i]);
			}
			printf("\n");
		}
	}
	return 0;
}

#include<stdio.h>//蛇形填数
#define MAX 100
int a[MAX][MAX]={0};//½«Õû¸öÊý×é³õÊ¼»¯ÎªÁã
int main()
{
	int i=0,j;
	int n,count=1;
	scanf("%d",&n);
	j=n-1;
	while(count<=n*n)
	{       
		while(i<n&&a[i][j]==0)
		{
			a[i][j]=count++;
			i++;
		}
		while(j>0&&a[i-1][j-1]==0)
		{
			a[i-1][j-1]=count++;
			j--;
		}
		while(i>1&&a[i-2][j]==0)
		{
			a[i-2][j]=count++;
			i--;
		}
		while(j<n-2&&a[i-1][j+1]==0)
		{
			a[i-1][j+1]=count++;
			j++;
		}
	}
	for(i=0;i<n;i++)
	{
		for(j=0;j<n;j++)
		{
			printf("%3d",a[i][j]);
		}
		printf("\n");
	}
	return 0;
}

#include<stdio.h>//啥玩意
#include<math.h>
int main()
{
	int T;
	scanf("%d",&T);
	while(T--){
		int n;
		scanf("%d",&n);
		int a[301];
		int b[301]={0};
		int i,j,ret=1;
		for(i=0;i<n;i++){
			scanf("%d",&a[i]);
		}
		int count[301]={0};
		for(i=0;i<n-1;i++){
			b[i]=abs(a[i]-a[i+1]);
			if(b[i]>=300){
				ret=0;
				break;
			}
			else{
				count[b[i]]++;
			}
		}
		if(ret==0){
			printf("Not jolly\n");
		}
		else{
			for(i=1;i<n;i++){
				if(count[i]!=1){
				ret=0;
			}
		}
			if(ret==0){
				printf("Not jolly\n");
			}
			else{
				printf("Jolly\n");
			}
		}
	}
	return 0;
}

#include<stdio.h>//括号配对
#include<string.h> 
int main()
{
	int T;
	scanf("%d",&T);
	while(T--){
		char a[1002];
		scanf("%s",a);
		int i;
		int len;
		int j=0;
		int ret=1;
		len=strlen(a);
		for(i=0;i<len;i++){
			if(a[i]=='('){
				j++;
			}
			if(a[i]==')'){
				j--;
			}
			if(j<0){
				ret=0;
				break;
			}
		}
		if(j>0){
			ret=0;
		}
		if(ret==0){
			printf("No\n");
		}
		else{
			printf("Yes\n");
		}
	}
	return 0;
}

#include<stdio.h>//计算年份
typedef struct{
	int year;
	int month;
	int day;
}D;
int main()
{
	int T;
	scanf("%d",&T);
	while(T--){
		D date;
		scanf("%d%d%d",&date.year,&date.month,&date.day);
		int a[13]={0,31,28,31,30,31,30,31,31,30,31,30,31};
		if(date.year%4==0&&date.year%100!=0||date.year%400==0){
			a[2]=29;
		}
		int i,sum=0;
		for(i=1;i<date.month;i++){
			sum+=a[i];
		}
		sum+=date.day;
		printf("%d\n",sum);
	}
	return 0;
}

#include<stdio.h>//数据大搜索。
#include<math.h>
#include<string.h>
int main()
{
	int T;
	scanf("%d",&T);
	getchar();
	while(T--){
		char a[1002]={0},b[1002]={0};
		int cnt=0,i;
		long sum=0;
		fgets(a,1002,stdin);
		int len;
		len=strlen(a);
		for(i=0;i<len-1;i++){
			b[i]=a[len-i-2];
		}
		b[len-1]='\0'; 
		i=0;
		while(b[i]!='\0'){
			if(b[i]-'0'>=0&&b[i]-'0'<=9){
				sum+=(b[i]-'0')*pow(10,cnt);
				cnt++;
			}
			else{
				cnt=0;
			}
			i++;
		}
		printf("%ld\n",sum);
	}
	return 0;
}

#include<cstdio>//第一道用c++做的题。挺方便的。
#include<iostream>
using namespace std;
int main()
{
	int T;
	scanf("%d",&T);
	while(T--){
		char ch;
		int cnt;
		int i,exist=1;
		string s;
		cin>>s;
		int len;
		len=s.length();
		if(len%3==1)
			cnt=len%3;
		else if(len%3==2)
			cnt=len%3-2;
		else
			cnt=len%3-1;
		for(i=0;i<4;i++){
			if(s[i]==','){
				exist=0;
				cnt=i;
				break;
			}
		}
		if(exist==0){
			for(i=0;i<len;i++){
				if(s[i]==','){
					continue;
				}
				else{
					printf("%c",s[i]);
				}
			}
		}
		else{
			int count=0;
			for(i=0;i<len;i++){
				printf("%c",s[i]);
				if(cnt%3==1&&count<(len-1)/3){
					printf(",");
					count++;
				}
				cnt++;
			}
		}
		printf("\n");
		
	}
	return 0;
}

#include<stdio.h>//加密
#include<string.h>
int main()
{
	//先读入密钥m，然后读入字符串，计算字符串长度
	///4的大小，判断如果整除就用
	//a[len/4+1][m],then assign value;if isn't整除 ,
	//就a[len/4+1+1][m]然后用e填剩下的m*(len/4+1)-len
	//个空，然后输出，用一个find函数找最大的，找到了就把它的数组值判为0，返回这个数组列数。
	char a[21]={0};
	scanf("%s",a);//输入DBCA 
	int len1;
	len1=strlen(a);
	char ch;
	char s[1010]={0};//输入字符串 
	int i=0;
	getchar();
	while((ch=getchar())!='\n'){//去掉空格 
		if(ch==' '){
			continue;
		}
		i++;
		s[i]=ch;//s从s【1】开始 
	}
	int length_string=i;//输入字符串的长度 
	int row;
	if(i%len1==0){
		row=length_string/len1+1;//第一行用来放密钥 
	}
	else{
		row=length_string/len1+2;
	}
	char b[row][len1];
	int j,cnt=1;
	for(i=1;i<row;i++){//把大写的录进字符串 
		for(j=0;j<len1;j++){
			if(s[cnt]-'a'>=0){
				s[cnt]=s[cnt]+'A'-'a';
			}
			b[i][j]=s[cnt];
			if(cnt==length_string){
				break;
			}
			cnt++;
		}
	}
	if(length_string%len1!=0){
		for(i=len1-1;i>=length_string%len1;i--){//补E 
			b[row-1][i]='E';
		}
	} 
	int rank[301]={0};
	for(i=0;i<len1;i++){//第一行密匙，哈希排序 
		rank[(int)a[i]]++;//可能有相同字符 
	}
	int pp,rr=0,t;
	for(i=0;i<=300;i++){
		if(rank[i]>0){
			while(rank[i]--){
				for(pp=0;pp<len1;pp++){
					if((int)a[pp]==i){
						t=pp;
						a[pp]=0;
						break;
					}
				}
				for(j=1;j<row;j++){ 
					printf("%c",b[j][t]);
				}
			}
		}
	}
	return 0;
}
