#include<stdio.h>//中间数
int main()
{
	int N;
	scanf("%d",&N);
	while(N>0){
		int a,b,c,t;
		scanf("%d%d%d",&a,&b,&c);
		if(a>b){
			t=a;a=b;b=t;
		}
		if(a>c){
			t=a;a=c;c=t;
		}
		if(b>c){
			t=b;b=c;c=t;
		}
		if(a!=b&&b!=c){
			printf("%d\n",b);
		}
		else{
			printf("%d\n",c);
		}
		N--;
	}
	return 0;
}

#include<stdio.h>//跳水打分求平均
int main()
{
	int T;
	scanf("%d",&T);
	while(T--){
		int n;
		int i;
		scanf("%d",&n);
		double a[100];
		double max,min,sum=0;
		for(i=0;i<n;i++){
			scanf("%lf",&a[i]);
		}
		max=a[0];
		min=a[0];
		sum=a[0];
		for(i=1;i<n;i++){
			if(a[i]>max){
				max=a[i];
			}
			if(a[i]<min){
				min=a[i];
			}
			sum=sum+a[i];
		}
		sum=sum-min-max;
		sum=sum/(n-2);
		printf("%.2f\n",sum);
	}
	return 0;
}

#include<stdio.h>//能否重组
#include<string.h>
int main()
{
	int T;
	scanf("%d",&T);
	while(T--){
		int cnta[10]={0,};
		int cntb[10]={0,};
		char a[200],b[200];
		scanf("%s",a);
		scanf("%s",b);
		int lengtha,lengthb;
		int ret=1,i;
		lengtha=strlen(a);
		lengthb=strlen(b);
		for(i=0;i<lengtha;i++){
			cnta[a[i]-'0']++;
		}
		for(i=0;i<lengthb;i++){
			cntb[b[i]-'0']++;
		}
		for(i=1;i<10;i++){
			if(cnta[i]!=cntb[i]){
				ret=0;
				break;
			}
		}
		if(ret==1){
			printf("Yes\n");
		}
		else if(ret==0){
			printf("No\n");
		}
		
		
	}
	return 0;
}

#include<stdio.h>//开灯问题
#include<string.h>
int main()
{
	int T;
	scanf("%d",&T);
	while(T--){
		int n,k;
		int a[1002]={0,};
		scanf("%d%d",&n,&k);
		int i,j;
		for(i=1;i<=k;i++){
			j=1;
			while(i*j<=n){
				a[i*j]++;
				j++;
			}
		}
		for(i=1;i<=n;i++){
			if(a[i]%2==1){
				printf("%d ",i);
			}
		}
		printf("\n");
	}
	return 0;
}
