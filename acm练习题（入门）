#include<stdio.h>//中间数
int main()
{
	int N;
	scanf("%d",&N);
	while(N>0){
		int a,b,c,t;
		scanf("%d%d%d",&a,&b,&c);
		if(a>b){
			t=a;a=b;b=t;
		}
		if(a>c){
			t=a;a=c;c=t;
		}
		if(b>c){
			t=b;b=c;c=t;
		}
		if(a!=b&&b!=c){
			printf("%d\n",b);
		}
		else{
			printf("%d\n",c);
		}
		N--;
	}
	return 0;
}
